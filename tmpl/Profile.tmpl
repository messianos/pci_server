
<% c++ #include "ViewContent.h" %>

<% skin pci_skin %>
	<% view profile_view uses ViewContent::ProfileContent extends template_view %>
	
	
		<% template css_links() %>
			<% include template_view::css_links() %>
			<link href="/css/header_bar.css" rel="stylesheet" type="text/css" />
			<link href="/css/navigation_menu.css" rel="stylesheet" type="text/css" />
		<% end template %>
	
	
		<% template page_content() %>
			<% include template_view::header_bar() %>
			<% include template_view::navigation_menu() %>
			
			<script type="text/javascript">
				$('#pci_navigation_menu_profile_button').addClass('active');
			</script>
			
			<div id="main_content_container">
				<div class="pci_box_2" >
					Nombre de Usuario: <%= user->user_name %><br />
					Nombre: <%= user->first_name %><br />
					Apellido: <%= user->last_name %><br />
					Sexo: <%= user->genre %><br />
					Fecha de Nacimiento: <%= user->birth_date->day %>/<%= user->birth_date->month %>/<%= user->birth_date->year %><br />
					E-mail: <%= user->email %><br />
					Fecha de registro: <%= user->sign_up_datetime %><br />
				</div>
			</div>
		<% end template %>
		
		
		<% template render_input() %>
	
		<% end template %>
	
	<% end view %>
<% end skin %>
